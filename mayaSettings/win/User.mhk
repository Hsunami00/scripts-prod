runTimeCommand
	-label ""
	-annotation "Toggle between select and pan modes"
	-longAnnotation ""
	-category "Custom Scripts"
	-image ""
	-tags ""
	-keywords ""
	-plugin ""
	-helpUrl ""
	-hotkeyCtx ""
	-commandLanguage "python"
	-command ("import pymel.core as pm\ntrack_context = \"trackSuperContext\"\nselect_context = \"selectSuperContext\"\n\nif pm.context.currentCtx() == track_context:\n    pm.general.setToolTo(select_context)\nelse:\n    pm.general.setToolTo(track_context)")
	TogglePanContext;

