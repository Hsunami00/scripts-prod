runTimeCommand
	-label ""
	-annotation ""
	-longAnnotation ""
	-category "Custom Scripts"
	-image ""
	-tags ""
	-keywords ""
	-plugin ""
	-helpUrl ""
	-hotkeyCtx ""
	-commandLanguage "python"
	-command ("if cmds.getPanel( to=cmds.getPanel(withFocus=1) ):\n    currentPanel = cmds.getPanel(withFocus=1)\n    \nif cmds.modelEditor(currentPanel, q=1, jx=1):\n    cmds.modelEditor(currentPanel, edit=1, jx=0)\n    cmds.modelEditor(currentPanel, edit=1, j=0)\nelse:\n    cmds.modelEditor(currentPanel, edit=1, jx=1)\n    cmds.modelEditor(currentPanel, edit=1, j=1)")
	ToggleJointXRayViewVis;

